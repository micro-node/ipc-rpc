  "devDependencies": {
    "async": "^1.5.1",
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "ghooks": "^1.0.1",
    "mocha": "^2.3.4"
